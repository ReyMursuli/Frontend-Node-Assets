{
  "name": "FrontendNuxt",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test:component": "npx vitest run app/tests/",
    "test:e2e": "npx playwright test app/tests/e2e/main.spec.ts",
    "test:e2e:headed": "npx playwright test app/tests/e2e/main.spec.ts --headed",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:debug": "npx playwright test app/tests/e2e/main.spec.ts --debug",
    "test:e2e:install": "npx playwright install",
    "test:all": "npx vitest run app/tests/"
  },
  "dependencies": {
    "@nuxt/image": "^2.0.0",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/seo": "^3.3.0",
    "@sidebase/nuxt-auth": "^1.1.1",
    "@tailwindcss/vite": "^4.1.16",
    "@vueuse/core": "^10.11.1",
    "nuxt": "^4.2.0",
    "nuxt-storage": "^1.2.2",
    "tailwindcss": "^4.1.16",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.23.0",
    "@playwright/test": "^1.58.1",
    "@types/node": "^25.0.3",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.3.9",
    "vitest": "^4.0.18"
  }
}
